#: original/man1/gawk.1:218
#, no-wrap
msgid "B<-W lint-old>"
msgstr ""

#. type: TP
#: original/man1/gawk.1:221
#, no-wrap
msgid "B<-\\^-lint-old>"
msgstr ""

#. type: Plain text
#: original/man1/gawk.1:227
msgid ""
"Provide warnings about constructs that are not portable to the original "
"version of Unix I<awk>."
msgstr ""

