msgid "lspci"
msgstr ""

#. type: TH
#: original/man8/lspci.8:1 original/man8/setpci.8:1 original/man8/update-pciids.8:1
#, no-wrap
msgid "04 January 2003"
msgstr ""

#. type: TH
