#. type: Plain text
#: build/C/man3/strftime.3:540
#, no-wrap
msgid ""
"#include E<lt>time.hE<gt>\n"
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"
msgstr ""

#. type: Plain text
#: build/C/man3/strftime.3:547
#, no-wrap
msgid ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    char outstr[200];\n"
"    time_t t;\n"
"    struct tm *tmp;\n"
msgstr ""

#. type: Plain text
#: build/C/man3/strftime.3:554
#, no-wrap
msgid ""
"    t = time(NULL);\n"
