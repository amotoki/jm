#: build/C/man2/execve.2:763
msgid ""
"B<chmod>(2), B<execveat>(2), B<fork>(2), B<ptrace>(2), B<execl>(3), "
"B<fexecve>(3), B<getopt>(3), B<credentials>(7), B<environ>(7), "
"B<path_resolution>(7), B<ld.so>(8)"
