#, no-wrap
msgid "2012-07-13"
msgstr ""

#. type: Plain text
#: build/C/man2/get_thread_area.2:11
msgid "get_thread_area - get a thread-local storage (TLS) area"
msgstr ""

#. type: Plain text
#: build/C/man2/get_thread_area.2:15 build/C/man2/set_thread_area.2:15
msgid "B<#include E<lt>asm/ldt.hE<gt>>"
