"if ((fd = open(block_device, O_RDONLY, 0)) == -1)\n"
"    err(1, \"%s\", block_device);\n"
msgstr ""
"if ((fd = open(raw_device, O_RDONLY, 0)) == -1)\n"
"    warnx(\"%s: %s: trying the block device\",\n"
"            raw_device, strerror(errno));\n"
"if ((fd = open(block_device, O_RDONLY, 0)) == -1)\n"
"    err(1, \"%s\", block_device);\n"

