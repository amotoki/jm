"B<% >I<cat tel>\n"
"maja    0501-1136285\n"
"peter   0136-7399214\n"
"B<% >I<cp tel tel2>\n"
"B<% >I<ls -l>\n"
"total 3\n"
"drwxr-xr-x   2 aeb       1024 Aug  6 23:51 bin\n"
"-rw-r--r--   1 aeb         37 Aug  6 23:52 tel\n"
"-rw-r--r--   1 aeb         37 Aug  6 23:53 tel2\n"
"B<% >I<mv tel tel1>\n"
"B<% >I<ls -l>\n"
"total 3\n"
"drwxr-xr-x   2 aeb       1024 Aug  6 23:51 bin\n"
