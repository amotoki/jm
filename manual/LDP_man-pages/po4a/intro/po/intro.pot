#: build/C/man7/man.7:315
msgid "\\&.TH I<...>"
msgstr ""

#. type: Plain text
#: build/C/man7/man.7:317
msgid "I<(later in the page)>"
msgstr ""

#. type: Plain text
#: build/C/man7/man.7:319
msgid "This software comes from the"
msgstr ""

#. type: Plain text
