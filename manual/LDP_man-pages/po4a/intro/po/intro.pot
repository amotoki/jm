msgstr ""

#. type: Plain text
#: build/C/man7/mdoc.samples.7:1642
msgid "Report name."
msgstr ""

#. type: It
#: build/C/man7/mdoc.samples.7:1642
#, no-wrap
msgid "Li .%T"
msgstr ""

#. type: Plain text
#: build/C/man7/mdoc.samples.7:1644
msgid "Title of article."
msgstr ""

#. type: It
