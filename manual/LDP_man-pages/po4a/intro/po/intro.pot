"\\&.Fa \"int type\"\n"
"\\&.Fa \"char *data\"\n"
"\\&.Fa \"int datalen\"\n"
"\\&.Fa \"struct rrec *newrr\"\n"
"\\&.Fa \"char *buf\"\n"
"\\&.Fa \"int buflen\"\n"
"\\&.Fc\n"
msgstr ""

