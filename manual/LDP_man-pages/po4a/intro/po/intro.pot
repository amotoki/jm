msgstr ""

#. type: Plain text
#: build/C/man7/mdoc.samples.7:873
msgid "E<.Ar file1>"
msgstr ""

#. type: It
#: build/C/man7/mdoc.samples.7:873
#, no-wrap
msgid "Li \\&.Ar file1\\ ."
msgstr ""

#. type: Plain text
#: build/C/man7/mdoc.samples.7:875
msgid "E<.Ar file1>."
msgstr ""

#. type: It
