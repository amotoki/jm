msgstr ""

#. type: Plain text
#: build/C/man3/fgetgrent.3:72
#, no-wrap
msgid ""
"struct group {\n"
"    char   *gr_name;        /* group name */\n"
"    char   *gr_passwd;      /* group password */\n"
"    gid_t   gr_gid;         /* group ID */\n"
"    char  **gr_mem;         /* group members */\n"
"};\n"
msgstr ""

