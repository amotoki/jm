msgstr ""

#. type: TP
#: build/C/man1/iconv.1:100
#, no-wrap
msgid "B<--verbose>"
msgstr ""

#. type: Plain text
#: build/C/man1/iconv.1:104
msgid "Print progress information on standard error when processing multiple files."
msgstr ""

