.TH CANONICALIZE_FILE_NAME 3 2005-07-14 "GNU" "Linux Programmer's Manual"
.\"O .SH NAME
.SH 名前
.\"O canonicalize_file_name \-  return the canonicalized filename
canonicalize_file_name \- 正規化されたファイル名を返す
.\"O .SH SYNOPSIS
.SH 書式
.B #define _GNU_SOURCE
.br
.B #include <stdlib.h>
.sp
.BI "char *canonicalize_file_name(const char *" path ");"
.\"O .SH DESCRIPTION
.SH 説明
.\"O The call
.\"O .I canonicalize_file_name(path)
.\"O is equivalent to the call
.\"O .IR "realpath(path,\ NULL)" .
.I canonicalize_file_name(path)
は
.I "realpath(path,\ NULL)"
と等価である。
.\"O .SH "CONFORMING TO"
.SH 準拠
.\"O The function is a GNU extension.
この関数は GNU による拡張である。
.\"O .SH "SEE ALSO"
.SH 関連項目
.BR realpath (3),
.BR feature_test_macros (7)
